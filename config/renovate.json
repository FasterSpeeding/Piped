{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "jsonata",
      "datasourceTemplate": "git-refs",
      "fileFormat": "toml",
      "managerFilePatterns": ["/tracked_modules.toml/"],
      "matchStrings": ["[registry.($ref := $split(reference, '#', 2); { 'currentDigest': ($ref)[1], 'currentValue': ($ref)[0], 'depName': repository })]"]
    }
  ],
  "extends": [
    "config:best-practices",
    ":maintainLockFilesWeekly"
  ],
  "git-submodules": {
    "enabled": true
  },
  "mise": {
    "pinDigests": false
  },
  "osvVulnerabilityAlerts": true,
  "pinDigests": true,
  "timezone": "Europe/Dublin",
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
