{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "jsonata",
      "datasourceTemplate": "git-refs",
      "fileFormat": "toml",
      "managerFilePatterns": ["/tracked_modules.toml/"],
      "matchStrings": ["[{ 'currentDigest': digest, 'currentValue': branch, 'packageName': repository }]"]
    },
    {
      "currentValueTemplate": "master",
      "customType": "regex",
      "datasourceTemplate": "git-refs"
      "managerFilePatterns": ["/^tracked_modules.ini$/"],
      "matchStrings": ["PIPED_VERSION=(?<currentDigest>.*?)\\n"],
      "packageNameTemplate": "https://github.com/FasterSpeeding/Piped",
    }
  ],
  "extends": [
    "config:best-practices",
    ":maintainLockFilesWeekly"
  ],
  "git-submodules": {
    "enabled": true
  },
  "mise": {
    "pinDigests": false
  },
  "osvVulnerabilityAlerts": true,
  "pinDigests": true,
  "timezone": "Europe/Dublin",
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
