{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "customManagers": [
    {
      "customType": "jsonata",
      "fileFormat": "toml",
      "managerFilePatterns": ["/tracked_modules.toml/"],
      "matchStrings": ["[registries.{ 'currentValueTemplate': commit, 'depName': repository }]"],
      "datasourceTemplate": "git-refs"
    }
  ],
  "extends": [
    "config:best-practices",
    ":maintainLockFilesWeekly"
  ],
  "git-submodules": {
    "enabled": true
  },
  "mise": {
    "pinDigests": false
  },
  "osvVulnerabilityAlerts": true,
  "pinDigests": true,
  "timezone": "Europe/Dublin",
  "vulnerabilityAlerts": {
    "enabled": true
  }
}
